{
  "name": "vue-fft-visualizer",
  "version": "0.1.0",
  "description": "A high-performance WebGL-based FFT spectrum visualizer component for Vue 3",
  "type": "module",
  "main": "./dist/vue-fft-visualizer.umd.cjs",
  "module": "./dist/vue-fft-visualizer.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-fft-visualizer.js",
      "require": "./dist/vue-fft-visualizer.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "~5.6.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "fft",
    "spectrum",
    "visualizer",
    "audio",
    "webgl",
    "analyzer"
  ],
  "author": "Wouter Vernaillen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wvernaillen/vue-fft-visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/wvernaillen/vue-fft-visualizer/issues"
  },
  "homepage": "https://github.com/wvernaillen/vue-fft-visualizer#readme"
}
